@model ICollection<AWWW_lab1_gr1.Models.Author>
 
<h2>Authors List</h2>
<p><a asp-action="Add">Add Author</a></p>
<table class="table">
     <tr><th>First Name</th><th>Last Name</th></tr>
    @foreach (var element in Model)
    {
        <tr>
            <td>@element.FirstName</td>
            <td>@element.LastName</td>
        </tr>
    }
</table>