@using AWWW_lab1_gr1.Models
@model IEnumerable<League>
  
   <h1>League List</h1>
   <p><a asp-action="Add">Add League</a></p> 
   <table class="table">
     <tr><th>Name</th><th>Country</th><th>Level</th></tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Country</td>
            <td>@item.Level</td>
        </tr>
    }
   </table>