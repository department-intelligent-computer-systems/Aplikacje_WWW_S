@using AWWW_lab1_gr1.Models
@model IEnumerable<Position>
   

 <h2>Position List</h2>

<p ><a asp-action="Create">Add Position</a></p>
<table class="table">
    <tr><th>Name</th></tr>
    @foreach(var item in Model)
    {
       <tr>
            <td>@item.ID</td>
           <td>@item.Name</td>
       </tr>
    }

</table>