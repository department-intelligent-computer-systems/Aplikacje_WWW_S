@model AWWW_lab1_gr1.Models.Team
@{
    ViewBag.Title = "Create team";
}
<h1>@ViewBag.Title</h1>
<form asp-action="Create">
    <div>
        <label asp-for="Name">Name:</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label asp-for="Country">Country:</label>
        <input asp-for="Country" />
    </div>
    <div>
        <label asp-for="FoundingDate">FoundingDate:</label>
        <input asp-for="FoundingDate" />
    </div>
    <div>
        <label asp-for="LeagueId">League:</label>
        <select asp-for="LeagueId" asp-items="ViewBag.LeagueId"></select>
    </div>
</form>
