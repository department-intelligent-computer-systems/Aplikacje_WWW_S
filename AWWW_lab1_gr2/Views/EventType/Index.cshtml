@model IEnumerable<AWWW_lab1_gr2.Models.EventType>


<div class="main">
    <div class="container">
        <div class="title">
            <h1>@ViewBag.Title</h1>
        </div>
        <div class="authors">
            @foreach(var eventType in Model){
                <li>
                    <div asp-route-id="@eventType.EventTypeId">@eventType.Name</div> 
                </li>
            }
        </div>
        <div class="addEventType">
            <a asp-action="form" asp-controller="eventType">Dodaj rodzaj wydarzenia</a>
        </div>
    </div>
</div>